{
  "name": "paddy",
  "version": "1.1.1",
  "description": "Discord bot.",
  "main": "dist/main.js",
  "types": "types/main.d.ts",
  "scripts": {
    "start": "node .",
    "build": "tslint -p tsconfig.dist.json -c tslint.json && ttsc -p tsconfig.dist.json",
    "test": "mocha test/setup.ts test/**/*.spec.ts -r ts-node/register -r tsconfig-paths/register",
    "coverage": "nyc npm run test"
  },
  "keywords": [
    "discord",
    "bot",
    "warframe"
  ],
  "author": "Pavel Koch <kouks.koch@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@exteranto/core": "^3.2.0",
    "awi": "0.0.10",
    "aws-sdk": "^2.814.0",
    "discord.js": "^12.2.0",
    "dotenv": "^8.2.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/chai-as-promised": "^7.1.2",
    "@types/dotenv": "^8.2.0",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.13.5",
    "@types/sinon": "^9.0.0",
    "@types/string-similarity": "^3.0.0",
    "@zerollup/ts-transform-paths": "^1.7.17",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "mocha": "^7.1.2",
    "nyc": "^15.0.1",
    "sinon": "^9.0.2",
    "ts-mockito": "^2.5.0",
    "ts-node": "^8.10.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.2",
    "ttypescript": "^1.5.10",
    "typescript": "^3.8.3"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  }
}
